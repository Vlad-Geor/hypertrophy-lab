{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "baseUrl": ".",
    "paths": {
      "@ikigaidev/assets": ["libs/shared/assets/src/index.ts"],
      "@ikigaidev/carousel": ["libs/shared/ui/carousel/src/index.ts"],
      "@ikigaidev/fixture": ["libs/shared/fixture/src/index.ts"],
      "@ikigaidev/footer": ["libs/shared/layout/footer/src/index.ts"],
      "@ikigaidev/header": ["libs/shared/layout/header/src/index.ts"],
      "@ikigaidev/hl/daily-workout-overview": [
        "libs/hypertrophy-lab/workouts/feature/daily-workout-overview/src/index.ts"
      ],
      "@ikigaidev/hl/model": ["libs/hypertrophy-lab/model/index.ts"],
      "@ikigaidev/hl/ui": ["libs/hypertrophy-lab/ui/index.js"],
      "@ikigaidev/hl/ui/muscle-group-card": [
        "libs/hypertrophy-lab/ui/muscle-group-card/src/index.ts"
      ],
      "@ikigaidev/hl/ui/pipe": ["libs/hypertrophy-lab/ui/pipe/src/index.ts"],
      "@ikigaidev/icon": ["libs/shared/ui/icon/src/index.ts"],
      "@ikigaidev/icon-text": ["libs/shared/ui/icon-text/src/index.ts"],
      "@ikigaidev/model": ["libs/shared/ui/model/src/index.ts"],
      "@ikigaidev/navigation": ["libs/shared/layout/navigation/src/index.ts"],
      "@ikigaidev/test": ["libs/hypertrophy-lab/ui/test/src/index.ts"],
      "@ikigaidev/util": ["libs/shared/util/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
