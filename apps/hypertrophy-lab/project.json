{
  "name": "hypertrophy-lab",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "prefix": "hl",
  "sourceRoot": "apps/hypertrophy-lab/src",
  "tags": [],
  "targets": {
    "build": {
      "dependsOn": [],
      "executor": "@angular-devkit/build-angular:application",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/apps/hypertrophy-lab",
        "index": "apps/hypertrophy-lab/src/index.html",
        "browser": "apps/hypertrophy-lab/src/main.ts",
        "polyfills": [
          "zone.js"
        ],
        "tsConfig": "apps/hypertrophy-lab/tsconfig.app.json",
        "inlineStyleLanguage": "scss",
        "assets": [
          {
            "glob": "**/*",
            "input": "apps/hypertrophy-lab/public"
          },
          {
            "glob": "**/*",
            "input": "apps/hypertrophy-lab/src/assets",
            "output": "assets"
          },
          {
            "glob": "**/*",
            "input": "libs/shared/assets/src/assets",
            "output": "shared-assets"
          }
        ],
        "styles": [
          "@angular/material/prebuilt-themes/cyan-orange.css",
          "apps/hypertrophy-lab/src/styles.scss"
        ],
        "stylePreprocessorOptions": {
          "includePaths": ["apps/hypertrophy-lab/src/styles"]
        }
      },
      "configurations": {
        "production": {
          "budgets": [
            {
              "type": "initial",
              "maximumWarning": "500kb",
              "maximumError": "1mb"
            },
            {
              "type": "anyComponentStyle",
              "maximumWarning": "2kb",
              "maximumError": "4kb"
            }
          ],
          "outputHashing": "all",
          "fileReplacements": [
            {
              "replace": "libs/shared/config/src/lib/env.ts",
              "with":    "apps/hypertrophy-lab/src/environments/environment.prod.ts"
            }
          ]
        },
        "development": {
          "optimization": false,
          "extractLicenses": false,
          "sourceMap": true,
          "fileReplacements": [
            {
              "replace": "libs/shared/config/src/lib/env.ts",
              "with": "apps/hypertrophy-lab/src/environments/environment.ts"
            }
          ]
        }
      },
      "defaultConfiguration": "production"
    },
    "serve": {
      "executor": "@angular-devkit/build-angular:dev-server",
      "configurations": {
        "production": {
          "buildTarget": "hypertrophy-lab:build:production",
          "proxyConfig": "apps/hypertrophy-lab/proxy.conf.json"
        },
        "development": {
          "buildTarget": "hypertrophy-lab:build:development",
          "proxyConfig": "apps/hypertrophy-lab/proxy.conf.json"
        }
      },
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "hypertrophy-lab:build:development",
        "proxyConfig": "apps/hypertrophy-lab/proxy.conf.json"
      }
    },
    "extract-i18n": {
      "executor": "@angular-devkit/build-angular:extract-i18n",
      "options": {
        "buildTarget": "hypertrophy-lab:build"
      }
    },
    "lint": {
      "executor": "@nx/eslint:lint"
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "apps/hypertrophy-lab/jest.config.ts"
      }
    },
    "serve-static": {
      "executor": "@nx/web:file-server",
      "options": {
        "buildTarget": "hypertrophy-lab:build",
        "port": 4200,
        "staticFilePath": "dist/apps/hypertrophy-lab/browser",
        "spa": true
      }
    }
  }
}