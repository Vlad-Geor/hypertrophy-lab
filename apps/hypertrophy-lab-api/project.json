{
  "name": "hypertrophy-lab-api",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/hypertrophy-lab-api/src",
  "projectType": "application",
  "tags": [],
   "targets": {
     "generate-routes": {
      "executor": "@nx/workspace:run-commands",
      "options": {
        "command": "node scripts/generate-routes.mjs"
      },
      "inputs": [
        "{projectRoot}/src/routes/*.routes.ts",
        "{workspaceRoot}/scripts/generate-routes.mjs"
      ],
      "outputs": ["{projectRoot}/src/routes/manifest.ts"]
    },
    "build": {
      "executor": "@nx/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "defaultConfiguration": "production",
      "options": {
        "platform": "node",
        "outputPath": "dist/apps/hypertrophy-lab-api",
        "format": ["esm"],
        "bundle": true,
        "target": "node20",
        "main": "apps/hypertrophy-lab-api/src/main.ts",
        "tsConfig": "apps/hypertrophy-lab-api/tsconfig.app.json",
        "assets": ["apps/hypertrophy-lab-api/src/assets"],
        "generatePackageJson": true,
        "esbuildOptions": {
          "sourcemap": true,
          "outExtension": {
            ".js": ".js"
          }
        }
      },
      "configurations": {
        "development": {
          "esbuildOptions": {
            "sourcemap": true,
            "outExtension": {
              ".js": ".js"
            }
          }
        },
        "production": {
          "esbuildOptions": {
            "sourcemap": false,
            "outExtension": {
              ".js": ".js"
            }
          }
        }
      }
    },
    "serve": {
      "executor": "@nx/js:node",
      "defaultConfiguration": "development",
      "options": {
        "buildTarget": "hypertrophy-lab-api:build:development",
        "watch": true,
        "runBuildTargetDependencies": false
      },
      "configurations": {
        "development": {
          "buildTarget": "hypertrophy-lab-api:build:development"
        },
        "production": {
          "buildTarget": "hypertrophy-lab-api:build:production"
        }
      }
    }
  }
}
